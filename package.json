    "jquery": "^3.1.1",
    "laravel-mix": "^1.5.1",
    "lodash": "^4.17.4",
    "vue": "^2.5.2"
  },
  "dependencies": {
    "bower": "^1.8.2",
    "gulp": "^3.9.1",
    "minimatch": "^3.0.4",
    "npm": "^6.0.1"
